<?xml version='1.0' encoding='utf-8'?>
<widget id="io.github.cedced19.tempspartour" version="0.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Temps par tour</name>
    <description>
        Une application pour obtenir rapidement le temps par tour.
    </description>
    <author email="cedced19@gmail.com" href="http://cedced19.github.io">
        CÃ©dric JUNG
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <preference name="StatusBarBackgroundColor" value="#bdc3c7" />
</widget>
